<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper  namespace=" com.example.demo222.mapper.RegisterMapper">

	
	<insert id="insertUser" parameterType="com.example.demo222.model.User">
	insert into login
	(id,username,password)values(#{id},#{username},#{password})
	</insert>
	<select id="selectUserByUsername" parameterType="java.lang.String" resultType="com.example.demo222.model.User">
	select * from login
	where username= #{username}
	
	</select>
	<select id="selectUserByPassword" parameterType="java.lang.String" resultType="com.example.demo222.model.User">
	select * from login
	where password= #{password}
	
	</select>
	
</mapper>